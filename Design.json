{
  "designSystem": {
    "name": "Modern Admin Dashboard",
    "version": "1.0.0",
    "description": "A clean, modern admin interface design system with emphasis on data tables and management interfaces"
  },
  "colorPalette": {
    "primary": {
      "blue": "#4F46E5",
      "blueHover": "#4338CA",
      "blueLight": "#E0E7FF"
    },
    "neutrals": {
      "darkNavy": "#1E293B",
      "mediumGray": "#64748B",
      "lightGray": "#F8FAFC",
      "white": "#FFFFFF",
      "borderGray": "#E2E8F0"
    },
    "status": {
      "success": "#10B981",
      "successLight": "#DCFCE7",
      "inactive": "#6B7280",
      "inactiveLight": "#F3F4F6"
    }
  },
  "typography": {
    "fontFamily": "Inter, -apple-system, BlinkMacSystemFont, sans-serif",
    "scales": {
      "heading1": {
        "fontSize": "24px",
        "fontWeight": "600",
        "lineHeight": "1.2"
      },
      "heading2": {
        "fontSize": "18px",
        "fontWeight": "600",
        "lineHeight": "1.3"
      },
      "body": {
        "fontSize": "14px",
        "fontWeight": "400",
        "lineHeight": "1.4"
      },
      "small": {
        "fontSize": "12px",
        "fontWeight": "400",
        "lineHeight": "1.3"
      }
    }
  },
  "layout": {
    "grid": {
      "containerMaxWidth": "1200px",
      "gutter": "24px",
      "columns": 12
    },
    "sidebar": {
      "width": "240px",
      "backgroundColor": "#1E293B",
      "position": "fixed",
      "height": "100vh"
    },
    "mainContent": {
      "marginLeft": "240px",
      "padding": "24px",
      "backgroundColor": "#F8FAFC"
    }
  },
  "components": {
    "navigationSidebar": {
      "structure": {
        "backgroundColor": "#1E293B",
        "width": "240px",
        "padding": "16px 0"
      },
      "items": {
        "padding": "12px 24px",
        "borderRadius": "6px",
        "margin": "0 12px 4px 12px",
        "iconSize": "20px",
        "iconColor": "#94A3B8",
        "textColor": "#CBD5E1",
        "hoverBackground": "#334155"
      },
      "logo": {
        "position": "top",
        "padding": "20px 24px",
        "borderBottom": "1px solid #334155"
      }
    },
    "dataTable": {
      "structure": {
        "backgroundColor": "#FFFFFF",
        "borderRadius": "8px",
        "border": "1px solid #E2E8F0",
        "overflow": "hidden"
      },
      "header": {
        "backgroundColor": "#F8FAFC",
        "padding": "12px 16px",
        "borderBottom": "1px solid #E2E8F0",
        "fontWeight": "500",
        "fontSize": "14px",
        "color": "#374151"
      },
      "rows": {
        "padding": "16px",
        "borderBottom": "1px solid #F1F5F9",
        "hoverBackground": "#F8FAFC"
      },
      "cells": {
        "verticalAlign": "middle",
        "textAlign": "left"
      }
    },
    "buttons": {
      "primary": {
        "backgroundColor": "#4F46E5",
        "color": "#FFFFFF",
        "padding": "8px 16px",
        "borderRadius": "6px",
        "fontSize": "14px",
        "fontWeight": "500",
        "border": "none",
        "hoverBackground": "#4338CA"
      },
      "secondary": {
        "backgroundColor": "#F8FAFC",
        "color": "#374151",
        "padding": "8px 16px",
        "borderRadius": "6px",
        "fontSize": "14px",
        "fontWeight": "500",
        "border": "1px solid #E2E8F0",
        "hoverBackground": "#F1F5F9"
      },
      "icon": {
        "size": "32px",
        "borderRadius": "6px",
        "backgroundColor": "transparent",
        "border": "none",
        "color": "#6B7280",
        "hoverBackground": "#F3F4F6"
      }
    },
    "badges": {
      "active": {
        "backgroundColor": "#DCFCE7",
        "color": "#166534",
        "padding": "4px 8px",
        "borderRadius": "12px",
        "fontSize": "12px",
        "fontWeight": "500"
      },
      "inactive": {
        "backgroundColor": "#F3F4F6",
        "color": "#6B7280",
        "padding": "4px 8px",
        "borderRadius": "12px",
        "fontSize": "12px",
        "fontWeight": "500"
      }
    },
    "statusIndicators": {
      "success": {
        "backgroundColor": "#10B981",
        "size": "8px",
        "borderRadius": "50%"
      },
      "inactive": {
        "backgroundColor": "#6B7280",
        "size": "8px",
        "borderRadius": "50%"
      }
    },
    "searchInput": {
      "backgroundColor": "#FFFFFF",
      "border": "1px solid #E2E8F0",
      "borderRadius": "6px",
      "padding": "8px 12px",
      "fontSize": "14px",
      "placeholder": "#9CA3AF",
      "focusBorder": "#4F46E5"
    },
    "dropdown": {
      "backgroundColor": "#FFFFFF",
      "border": "1px solid #E2E8F0",
      "borderRadius": "6px",
      "padding": "8px 12px",
      "fontSize": "14px",
      "minWidth": "120px"
    },
    "pagination": {
      "container": {
        "display": "flex",
        "alignItems": "center",
        "justifyContent": "space-between",
        "padding": "16px"
      },
      "buttons": {
        "size": "32px",
        "borderRadius": "4px",
        "border": "1px solid #E2E8F0",
        "backgroundColor": "#FFFFFF",
        "color": "#374151",
        "activeBg": "#4F46E5",
        "activeColor": "#FFFFFF"
      }
    }
  },
  "spacing": {
    "xs": "4px",
    "sm": "8px",
    "md": "12px",
    "lg": "16px",
    "xl": "24px",
    "xxl": "32px"
  },
  "borderRadius": {
    "small": "4px",
    "medium": "6px",
    "large": "8px",
    "pill": "12px",
    "circle": "50%"
  },
  "shadows": {
    "subtle": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
    "medium": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -1px rgba(0, 0, 0, 0.06)",
    "large": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)"
  },
  "patterns": {
    "tableRowStructure": {
      "description": "Standard table row with consistent spacing and alignment",
      "elements": [
        "Icon/status indicator (left)",
        "Primary text/name",
        "Secondary text/description",
        "Numerical data/count",
        "Status badge/dropdown",
        "Action buttons (right)"
      ]
    },
    "headerPattern": {
      "description": "Page header with title and primary action",
      "elements": [
        "Breadcrumb navigation",
        "Page title",
        "Export/print actions",
        "Primary CTA button"
      ]
    },
    "filterSection": {
      "description": "Search and filter controls above data tables",
      "elements": [
        "Search input with icon",
        "Filter dropdown",
        "Results count display"
      ]
    }
  },
  "interactions": {
    "hover": {
      "tableRows": "Light background change (#F8FAFC)",
      "buttons": "Slight color darkening",
      "navigationItems": "Background highlight"
    },
    "focus": {
      "inputs": "Blue border highlight",
      "buttons": "Outline ring"
    },
    "active": {
      "navigationItems": "Darker background with left border accent",
      "buttons": "Pressed state with darker background"
    }
  },
  "iconSystem": {
    "style": "Outline/stroke icons",
    "weight": "1.5px stroke",
    "sizes": {
      "small": "16px",
      "medium": "20px",
      "large": "24px"
    },
    "usage": {
      "navigation": "20px icons with consistent spacing",
      "actions": "16px icons in buttons",
      "status": "16px icons for indicators"
    }
  },
  "responsiveness": {
    "breakpoints": {
      "mobile": "768px",
      "tablet": "1024px",
      "desktop": "1200px"
    },
    "sidebarBehavior": {
      "mobile": "Collapsible/overlay",
      "desktop": "Fixed sidebar"
    },
    "tableBehavior": {
      "mobile": "Horizontal scroll",
      "desktop": "Full width display"
    }
  }
}